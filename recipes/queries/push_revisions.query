from: repo
groupby:
    - {name: pushid, value: push.id}
select:
    - {name: rev, value: changeset.id, aggregate: union}
    - {name: parents, value: parents, aggregate: union}
sort: push.id
where:
    and:
        - eq: {branch.name: {$eval: branch}}
        - gte: [push.id, {$eval: from_push}]
        - lte: [push.id, {$eval: to_push}]
limit: 10000
format: table
